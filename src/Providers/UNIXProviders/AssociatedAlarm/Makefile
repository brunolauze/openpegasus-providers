ifeq ($(OS), aix)
	SOURCES += UNIX_AssociatedAlarm_AIX.cpp
endif
ifeq ($(OS), darwin)
	SOURCES += UNIX_AssociatedAlarm_DARWIN.cpp
endif
ifeq ($(OS), freebsd)
	SOURCES += UNIX_AssociatedAlarm_FREEBSD.cpp
endif
ifeq ($(OS), hpux)
	SOURCES += UNIX_AssociatedAlarm_HPUX.cpp
endif
ifeq ($(OS), linux)
	SOURCES += UNIX_AssociatedAlarm_LINUX.cpp
endif
ifeq ($(OS), solaris)
	SOURCES += UNIX_AssociatedAlarm_SOLARIS.cpp
endif
ifeq ($(OS), tru64)
	SOURCES += UNIX_AssociatedAlarm_TRU64.cpp
endif
ifeq ($(OS), vms)
	SOURCES += UNIX_AssociatedAlarm_VMS.cpp
endif
ifeq ($(OS), win32)
	SOURCES += UNIX_AssociatedAlarm_WIN32.cpp
endif
ifeq ($(OS), zos)
	SOURCES += UNIX_AssociatedAlarm_ZOS.cpp
endif
ifeq (, $(filter aix,darwin,freebsd,hpux,linux,solaris,tru64,vms,win32,zos $(OS)))

else
	SOURCES += UNIX_AssociatedAlarm_STUB.cpp
endif

