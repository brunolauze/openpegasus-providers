LIBRARY = UNIX_AssociatedAlarmProvider
EXTRA_INCLUDES += -I/usr/local/include -I./..

DEFINES +=  -DPROVIDER_LIB_NS="UNIX_AssociatedAlarmLib" -DPROVIDER_LIB_NS_BEGIN="namespace UNIX_AssociatedAlarmLib {"

SOURCES = \
	../CIMHelper.cpp \
	UNIX_AssociatedAlarm.cpp \
